									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response contains required fields\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData).to.have.all.keys('bookingid', 'booking');\r",
									"    pm.expect(responseData.booking).to.be.an('object');\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Validate booking object structure and types\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData).to.have.property('bookingid').that.is.a('number');\r",
									"    \r",
									"    pm.expect(responseData).to.have.property('booking').that.is.an('object');\r",
									"    pm.expect(responseData.booking).to.have.property('firstname').that.is.a('string');\r",
									"    pm.expect(responseData.booking).to.have.property('lastname').that.is.a('string');\r",
									"    pm.expect(responseData.booking).to.have.property('totalprice').that.is.a('number');\r",
									"    pm.expect(responseData.booking).to.have.property('depositpaid').that.is.a('boolean');\r",
									"    \r",
									"    pm.expect(responseData.booking).to.have.property('bookingdates').that.is.an('object');\r",
									"    pm.expect(responseData.booking.bookingdates).to.have.property('checkin').that.is.a('string');\r",
									"    pm.expect(responseData.booking.bookingdates).to.have.property('checkout').that.is.a('string');\r",
									"    \r",
									"    pm.expect(responseData.booking).to.have.property('additionalneeds').that.is.a('string');\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"Response time is less than 2200ms\", function () {\r",
									"    pm.expect(pm.response.responseTime).to.be.below(2200);\r",
									"});\r",
									"\r",
									"\r",
									"pm.test(\"The totalprice must be a positive integer\", function () {\r",
									"    const responseData = pm.response.json();\r",
									"    \r",
									"    pm.expect(responseData).to.be.an('object');\r",
									"    pm.expect(responseData.booking.totalprice).to.be.a('number').and.to.be.above(0);\r",
									"});\r",
									""
